{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "functions": {
        "index.py": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "env": {
        "ENVIRONMENT": "production"
    }
}